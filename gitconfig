[credential]
	helper = osxkeychain

[color]
	ui = true

[push]
	default = current

[core]
	editor = vim
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes

[alias]
	lastchange = log -p -n 1
	trim = !git remote prune origin && git gc

[include]
	path = ~/.gitconfig.local

[rebase]
	autostash = true

[diff "json"]
	textconv = json_pp

[user]
	name = Brian van Burken
[url "git@github.com:"]
	insteadOf = https://github.com/
[commit]
	gpgsign = true
