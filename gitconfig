[credential]
	helper = osxkeychain

[color]
	ui = true

[push]
	default = current

[core]
	editor = vim
	excludesfile = ~/.gitignore
	attributesfile = /Users/briburke/.gitattributes

[alias]
	lastchange = log -p -n 1
	trim = !git remote prune origin && git gc

[include]
	path = ~/.gitconfig.local

[rebase]
	autostash = true
[diff "json"]
	textconv = json_pp
